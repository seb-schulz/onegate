# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type PubKeyCredParam {
  type: String!
  alg: Int!
}

type RelyingParty {
  name: String!
  id: String!
}

type CreateCredentialOptions {
  challenge: String!
  rp: RelyingParty!
  pubKeyCredParams: [PubKeyCredParam]!
  userID: String!
}

type User {
  PasskeyID: String!
}

type Query {
  redeemToken: String!
  me: User
}

type Mutation {
 createUser: CreateCredentialOptions!
}
